{
  "name": "commented.tech",
  "private": true,
  "description": "A place for my personal brain dumps, comments, and thoughts",
  "version": "0.1.0",
  "author": "Tom Gallacher <tgallacher@users.noreply.github.com>",
  "scripts": {
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "format": "prettier --config ./.prettierrc.json --write 'src/**/*.js' 'src/**/*.md'",
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "bugs": {
    "url": "https://github.com/tgallacher/commented.tech/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.4.5",
    "@mdx-js/react": "^1.4.5",
    "@theme-ui/color": "^0.2.x",
    "@theme-ui/prism": "^0.2.x",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-changelog-context": "^1.0.2",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-mdx": "1.0.44",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-remove-generator": "^1.0.4",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-theme-ui": "^0.2.41",
    "gatsby-plugin-twitter": "^2.1.6",
    "gatsby-remark-autolink-headers": "^2.0.15",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-embedder": "^1.1.2",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "3",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.1.3",
    "moment": "^2.24.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "reading-time": "^1.2.0",
    "theme-ui": "^0.2.42",
    "typeface-ibm-plex-mono": "^0.0.61",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@emotion/babel-preset-css-prop": "^10.0.7",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.10",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "prettier": "^1.14.2",
    "prismjs": "^1.15.0",
    "styled-system": "^5.x.x"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbyjs/gatsby-starter-blog.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
